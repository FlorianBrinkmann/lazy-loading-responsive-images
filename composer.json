{
  "name": "florianbrinkmann/lazy-loading-responsive-images",
  "description": "Lazy loading plugin for WordPress \u0000 uses lazysizes.js.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Florian Brinkmann",
      "email": "kontakt@florianbrinkmann.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "FlorianBrinkmann\\LazyLoadResponsiveImages\\": "src/"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
    "phpcompatibility/php-compatibility": "^9.3"
  },
  "prefer-stable" : true,
  "require": {
    "masterminds/html5": "^2.9"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
